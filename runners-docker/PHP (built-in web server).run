{
    "cmd": [
        "php",
        "${debug?-d}", "${debug?xdebug.remote_enable=1}",
        "-S", "$ip:$port"
    ],
    "selector": "^.*\\.(php)$",
    "info": "Running PHP script $file",
    "cwd": "$project_path",
    "debugger": "xdebug",
    "debugport": 9000,
    "env": {
        "XDEBUG_CONFIG": "\"idekey=cloud9ide remote_host=localhost remote_port=9000 remote_mode=req\""
    }
}
